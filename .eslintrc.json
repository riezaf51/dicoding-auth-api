{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": "off",
        "indent": [
            "error",
            4
        ],
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off"
    }
}